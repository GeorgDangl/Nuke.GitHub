{
  "metadata": [
    {
      "src": [
        {
          "src": "src",
          "files": [
            "Nuke.GitHub/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "**/*.yml"
        ],
        "exclude": [
          "output/**/*",
          "GitVersion.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**.png"
        ]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "Nuke.GitHub Documentation",
      "_appFooter": "&copy; <a href=\"https://www.dangl-it.com\" target=\"_blank\">Dangl IT GmbH</a>",
      "_appLogoPath": "app-logo.png",
      "_appFaviconPath": "app-logo.png",
      "_enableSearch": true,
      "_gitUrlPattern": "github"
    },
    "dest": "output/docs",
    "template": [
      "default",
      "docstemplate"
    ]
  }
}
