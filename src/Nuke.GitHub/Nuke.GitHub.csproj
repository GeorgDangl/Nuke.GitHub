  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" />
    <PackageReference Include="Octokit" Version="0.29.0" />
    <PackageReference Include="Nuke.Common" Version="0.4.0" />
  </ItemGroup>

</Project>
